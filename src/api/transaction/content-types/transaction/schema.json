{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "paidAt": {
      "type": "datetime"
    },
    "orderedAt": {
      "type": "datetime"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "transaction"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "transaction"
    },
    "warrantyEndAt": {
      "type": "datetime"
    },
    "warrantyVoid": {
      "type": "boolean"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "paymentProof": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "vehicleBrand": {
      "type": "string",
      "maxLength": 100
    },
    "vehicleModel": {
      "type": "string",
      "maxLength": 100
    },
    "vehicleYear": {
      "type": "string",
      "maxLength": 4
    }
  }
}
