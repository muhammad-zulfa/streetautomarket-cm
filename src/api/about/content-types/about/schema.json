{
  "kind": "singleType",
  "collectionName": "abouts",
  "info": {
    "singularName": "about",
    "pluralName": "abouts",
    "displayName": "About",
    "description": "Write about yourself and the content you create"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "maxLength": 15
    },
    "dealershipPhone": {
      "type": "string",
      "maxLength": 16
    },
    "description": {
      "type": "string",
      "maxLength": 200
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.rich-text"
      ]
    },
    "address": {
      "type": "string",
      "maxLength": 255
    },
    "location": {
      "type": "customField",
      "customField": "plugin::strapi-location-picker.location-picker",
      "options": {
        "info": true
      }
    }
  }
}
